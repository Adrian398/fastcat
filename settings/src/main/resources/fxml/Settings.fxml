<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="500.0" prefWidth="700.0" spacing="10.0"
      stylesheets="@/css/Settings.css" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="settings.SettingsController">
  <HBox spacing="5.0" VBox.vgrow="ALWAYS">
    <ListView fx:id="listViewEntries" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="200.0"/>
    <StackPane fx:id="stackPaneContent" HBox.hgrow="ALWAYS"/>
  </HBox>
  <ButtonBar>
    <buttons>
      <Button onAction="#onOkAction" defaultButton="true" mnemonicParsing="false" text="OK"/>
      <Button onAction="#onCancelAction" cancelButton="true" mnemonicParsing="false" text="Cancel"/>
    </buttons>
  </ButtonBar>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
</VBox>
